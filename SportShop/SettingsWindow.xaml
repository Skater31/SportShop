<Window x:Class="SportShop.SettingsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SportShop"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        ResizeMode="CanMinimize"
        Title="Settings" Height="450" Width="400">
    <Grid>
        <Button x:Name="ButtonLogout" Content="Logout" HorizontalAlignment="Center" Margin="0,162,0,0" VerticalAlignment="Top" Click="ButtonLogout_Click"/>
        <Button x:Name="ButtonBack" Content="Back" HorizontalAlignment="Center" Margin="0,247,0,0" VerticalAlignment="Top" Click="ButtonBack_Click"/>

    </Grid>
</Window>
